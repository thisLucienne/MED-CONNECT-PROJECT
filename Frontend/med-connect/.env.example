# Configuration d'environnement pour Med-Connect Mobile

# URL du backend API
# Pour développement local avec Expo Go, utilisez l'IP de votre machine
# Exemple: REACT_NATIVE_API_URL=http://192.168.1.100:3000/api
REACT_NATIVE_API_URL=http://10.190.6.71:3000/api

# Timeout des requêtes API (en millisecondes)
REACT_NATIVE_API_TIMEOUT=10000

# Mode debug
REACT_NATIVE_DEBUG=true